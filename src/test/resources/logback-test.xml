<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <statusListener class="ch.qos.logback.core.status.NopStatusListener" />

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %thread %-5level %logger{0} - %message%n</pattern>
		</encoder>
	</appender>

 
    <logger name="com.alien.examples.webdriver" level="DEBUG"/>
    <logger name="com.alien.utils" level="DEBUG"/>
 

	<root level="ERROR">
       <!-- <appender-ref ref="FILE" /> -->
		<appender-ref ref="STDOUT" />
	</root>
</configuration>